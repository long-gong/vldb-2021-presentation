   "source": [
    "# pip install xxhash\n",
    "\n",
    "import xxhash \n",
    "\n",
    "NUM_BUCKETS = 15\n",
    "for v in [1, 2, 3, 4, 5, 6]:\n",
    "    v_bytes = v.to_bytes(4, 'big')\n",
    "    bi = xxhash.xxh32(v_bytes).intdigest() % NUM_BUCKETS\n",
    "    print(f'{v} => {bi}')"
   ]
